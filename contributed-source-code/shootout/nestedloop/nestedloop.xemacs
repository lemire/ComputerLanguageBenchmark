(defun main ()
  (let* ((arg (car command-line-args-left))
	 (n (if arg (string-to-number arg) 1))
	 (x 0))
    (dotimes (a n)
      (dotimes (b n)
	(dotimes (c n)
	  (dotimes (d n)
	    (dotimes (e n)
	      (dotimes (f n)
		(setq x (+ x 1))))))))
    (princ (format "%d\n" x))))
