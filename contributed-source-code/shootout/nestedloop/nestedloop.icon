procedure main(argv)
    local n, a, b, c, d, e, f, x
    n := argv[1] | 1;
    x := 0
    every a := 1 to n do
	every b := 1 to n do
	    every c := 1 to n do
		every d := 1 to n do
		    every e := 1 to n do
			every f := 1 to n do
			    x +:= 1
    write(x)
end
