(define (tak x y z)
  (if (>= y x)
      z
      (tak (tak (sub x 1) y z) (tak (sub y 1) z x) (tak (sub z 1) x y))))

(define (main)
  (set 'n (integer (last (main-args))))
  (println
    (format "%.1f" (tak (mul n 3.0) (mul n 2.0) (mul n 1.0) ))))

(main)
(exit)

