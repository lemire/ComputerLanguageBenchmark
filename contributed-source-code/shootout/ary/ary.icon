procedure main(argv)
    # local n, i, k, x, y, last
    n := argv[1] | 1
    x := list(n,0)
    y := list(n,0)
    every i := 1 to n do {
	x[i] := i
    }
    every k := 0 to 999 do {
	every i := n to 1 by -1 do {
	    y[i] +:= x[i]
	}
    }
    write(y[1], " ", y[n])
end
