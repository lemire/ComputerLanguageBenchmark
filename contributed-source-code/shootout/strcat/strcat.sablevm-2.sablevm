import java.io.*;
import java.util.*;

public class strcat {
    public static void main(String args[]) throws IOException {
	int n = Integer.parseInt(args[0]);
	String str = "";

	for (int i=0; i<n; i++) {
	    str += "hello\n";
	}

	System.out.println(str.length());
    }
}
